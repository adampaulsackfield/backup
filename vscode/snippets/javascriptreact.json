{
	// ! ARROW FUNCTIONS START
	"Arrow Function No Params": {
		"prefix": "af",
		"body": "() => $0",
		"description": "Expands into an arrow function, without params."
	},

	"Arrow Function Params": {
		"prefix": "afa",
		"body": "($1) => $0",
		"description": "Expands into an arrow function, with params."
	},
	"Arrow Function Return No Params": {
		"prefix": "afr",
		"body": ["() => {", "return $0", "}"],
		"description": "Epands "
	},
	"Arrow Function Return Params": {
		"prefix": "afar",
		"body": ["($1) => {", "return $0", "}"],
		"description": "Epands "
	},
	// ! ARROW FUNCTIONS END

	// ! REACT START
	"React Arrow Function Const Export": {
		"prefix": "rafce",
		"body": [
			"import './$TM_FILENAME_BASE.scss'",
			"",
			"const $TM_FILENAME_BASE = () => {",
			"",
			"  return <div>$TM_FILENAME_BASE</div>;",
			"",
			"};",
			"",
			"export default $TM_FILENAME_BASE"
		],
		"description": "Builds a react functional arrow component, named the same as the file"
	},
	// ! REACT END

	// ! ARROW FUNCTION START
	"Arrow Function": {
		"prefix": "aff",
		"body": ["const $1 = ($2) => {", "  return $3", "}"],
		"description": "Expands into an arrow function."
	},
	// ! ARROW FUNCTION END

	// ! IMPORT FROM START
	"Import From": {
		"prefix": "imp",
		"body": "import $1 from '$2'",
		"description": "Expands to import NAME from 'PLACE'"
	}
	// ! IMPORT FROM END
}
